    {
      "id": "B3UkJKZuc1XD2WvNer2j9",
      "type": "text",
      "x": 4431.696169387747,
      "y": 565.896296022956,
      "width": 914.9700552576497,
      "height": 350,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
